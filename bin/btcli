#!/usr/bin/env python
import logging
from bittensor_cli.cli import CLIManager


def main():
    try:
        manager = CLIManager()
        manager.run()
    except KeyboardInterrupt:
        print('KeyboardInterrupt')
    except RuntimeError as e:
        logging.error(f'RuntimeError: {e}')


if __name__ == '__main__':
    main()
